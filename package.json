{
	"name": "aws-2-vs",
	"displayName": "aws-2-vs",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:cloudwatchLogs",
		"onCommand:cloudwatchLogs.refreshGroups",
		"onCommand:cloudwatchLogs.showLogGroupSearch",
		"onCommand:cloudwatchLogs.clearGroupNameFilter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "cloudwatch-logs",
					"title": "Cloudwatch",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"cloudwatch-logs": [
				{
					"id": "cloudwatchLogs",
					"name": "Log Groups"
				}
			]
		},
		"commands": [
			{
				"command": "cloudwatchLogs.refreshGroups",
				"title": "Refresh",
				"icon": {
					"light": "media/light/icon-refresh.svg",
					"dark": "media/dark/icon-refresh.svg"
				}
			},
			{
				"command": "cloudwatchLogs.clearGroupNameFilter",
				"title": "Clear",
				"icon": {
					"light": "media/light/icon-clear.svg",
					"dark": "media/dark/icon-clear.svg"
				}
			},
			{
				"command": "cloudwatchLogs.showLogGroupSearch",
				"title": "Search LogGroup",
				"icon": {
					"light": "media/light/icon-search.svg",
					"dark": "media/dark/icon-search.svg"
				}
			},
			{
				"command": "cloudwatchLogs.refreshLog",
				"title": "Refresh log",
				"icon": {
					"light": "media/light/icon-refresh.svg",
					"dark": "media/dark/icon-refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "cloudwatchLogs.refreshGroups",
					"when": "view == cloudwatchLogs",
					"group": "navigation"
				},
				{
					"command": "cloudwatchLogs.clearGroupNameFilter",
					"when": "view == cloudwatchLogs",
					"group": "navigation"
				},
				{
					"command": "cloudwatchLogs.showLogGroupSearch",
					"when": "view == cloudwatchLogs",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "cloudwatchLogs.refreshLog",
					"when": "view == cloudwatchLogs",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"fp-ts": "^2.0.5",
		"io-ts": "^2.0.1"
	}
}
